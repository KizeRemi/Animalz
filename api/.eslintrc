{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended"
    ],
    "plugins": [
        "import"
    ],
    "rules": {
        "no-multiple-empty-lines": [2, {
            "max": 1
        }],
        "strict": 0,
        "semi": 2
    }
}
